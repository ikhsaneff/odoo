<odoo>
	<record id="custom_hr_applicant_view_form" model="ir.ui.view">
		<field name="name">Jobs - Recruitment Form</field>
		<field name="model">hr.applicant</field>
		<field name="inherit_id">hr_applicant_view_form</field>
		<field name="arch" type="xml"></field>
		<xpath expr="//page[@string='Application Summary']" position="after">
			<notebook position="inside">
				<page string="Interview 1">

				</page>
				<page string="Interview 2">

				</page>
			</notebook>
		</xpath>
	</record>
</odoo>